	Journal = {Eur. J. Biochem.},
	Keywords = {modeling, MAPK, oscillations, phosphorylation},
	Number = {6},
	Pages = {1583--1588},
	Title = {Negative feedback and ultrasensitivity can bring about oscillations in the mitogen-activated protein kinase cascades},
