	Pages = {3116--3120},
	Title = {Transfer of proteins from gels to diazobenzyl\-oxymethyl-paper and detection with antisera: a method for studying antibody specificity and antigen structure},
	Volume = {76},
	Year = {1979}}

@article{HeinrichWei2011,
	Abstract = {Continuous scatterplots and parallel coordinates are used to visualize multivariate data defined on a continuous domain. With the existing techniques, rendering such plots becomes prohibitively slow, especially for large scientific datasets. This paper presents a scalable and progressive rendering algorithm for continuous data plots that allows exploratory analysis of large datasets at interactive framerates. The algorithm employs splatting to produce a series of plots that are combined using alpha blending to achieve a progressively improving image. For each individual frame, splats are obtained by transforming Gaussian density kernels from the 3-D domain of the input dataset to the respective data domain. A closed-form analytic description of the resulting splat footprints is derived to allow pre-computation of splat textures for efficient GPU rendering. The plotting method is versatile because it supports arbitrary reconstruction or interpolation schemes for the input data and the splatting technique is scalable because it chooses splat samples independently from the size of the input dataset. Finally, the effectiveness of the method is compared to existing techniques regarding rendering performance and quality.},
	Author = {J. Heinrich and S. Bachthaler and D. Weiskopf},
	Date-Added = {2012-01-05 19:19:14 +0100},
	Date-Modified = {2013-03-11 13:12:28 +0100},
