	Doi = {10.1038/nature06115},
	Journal = {Nature},
	Keywords = {BrdU, cell-to-cell variability, asymmetric cell division, stem cells},
	Month = {Sept.},
	Number = {7159},
	Pages = {238--242},
	Rating = {5},
	Read = {1},
	Title = {Haematopoietic stem cells do not asymmetrically segregate chromosomes or retain BrdU},
	Volume = {449},
	Year = {2007},
