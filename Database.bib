	Month = {Nov.},
	Number = {Suppl 19},
	Pages = {S2},
	Rating = {5},
	Read = {1},
	Title = {{iVUN}: interactive Visualization of Uncertain biochemical reaction Networks},
	Volume = {14},
	Year = {2013},
