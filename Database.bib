	Doi = {10.1074/jbc.M805358200},
	Journal = {Journal of Biological Chemistry},
	Keywords = {PKD, inhibition, kinases, signaling, cell cycle},
	Month = {Nov.},
	Number = {48},
	Pages = {33516--33526},
	Rating = {4},
	Read = {1},
	Title = {Potent and selective disruption of {P}rotein {K}inase {D} functionality by a {B}enzoxoloazepinolone},
	Volume = {283},
