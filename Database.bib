	Rating = {4},
	Read = {1},
	Title = {Optimal design of dynamic experiments for improved estimation of kinetic parameters of thermal degradation},
	Url = {http://dx.doi.org/10.1016/j.jfoodeng.2007.02.006},
	Volume = {82},
	Year = {2007},
