@misc{HasenauerErb2011,
	Abstract = {Background: The treatment of cancer with therapeutics is a multi-scale problem, involving: (i) the distribution processes in the vascular system, the capillary bed and the tissue; and (ii) the initiation of intracellular signaling resulting in cell fate decisions. This fact has been appreciated for years but the development of mechanistic models spanning all these scales just begins. In particular, heterogeneity among cancer cells is a fact which is not considered in most models.  

Results: In this work a preliminary mathematical model for TRAIL-fusion protein treatment of lung carcinoma is developed. The multi-scale model we propose incorporates a mechanistic description of the distribution processes (i) and of the cell signaling (ii). The former takes the particular structure and topology of lung tissue into count, while the latter one respects heterogeneity of cancer cell populations.
Mathematically the overall system is described via two partial differential equation models coupled to a cell ensemble model. Both models are implemented in individual software packages, DuMuX respectively the Matlab Toolbox CellPoT, which are linked via a newly developed interface. This allows for a maximal degree of freedom and an independent improvement of the submodels.
To gain a deeper insight into the dynamics of the treatment process, several simulation studies are performed, allowing for the assessment of the relative importance of different unknown process parameters. Especially, the time scales of the individual processes and the improved effects of a TRAIL-fusion protein compared to normal TRAIL is analyzed. 

Conclusions: Advanced modeling methods and measurement data available for different process scales enabled the development of a mechanistic, partially validated multi-scale model of TRAIL treatment of lung carcinoma. This provides deeper insight into the effects of the treatment as well as the uncertainties of the prediction. Furthermore, it helps to select the most informative future experiments, which is particular complex in a multi-scale setting.},
	Author = {J. Hasenauer and K. Erbertseder and M. Doszczak and R. Helmig and P. Scheurich and F. Allg\"ower},
	Date-Added = {2011-09-05 12:55:48 +0200},
	Date-Modified = {2013-11-04 21:17:55 +0100},
	Howpublished = {Abstract in Proc. of 12th Int. Conf. Systems Biology (ICSB 2011), August 28 - September 1, Heidelberg/Mannheim, Germany},
	Keywords = {multi-scale modeling, sensitivity analysis, uncertainty analysis, dynamical systems},
	Month = {Aug.},
	Rating = {5},
	Read = {1},
	Title = {Towards a multi-scale model for the therapeutic action of TRAIL in lung carcinoma},
	Year = {2011},
	Bdsk-File-1 = {}}

@misc{HasenauerWal2011c,
	Abstract = {Background: Individual cells within a clonal population often show different responses upon a common stimulus. This is a fact widely appreciated but still difficult to study using model-based approaches. The study of population heterogeneity requires the development of population models, and knowledge about cell-to-cell variability. Unfortunately, so far no suitable methods are available to systematically infer information about cellular heterogeneity from common cell-biological measurement data.

Results: In this work cell population models are studied in which the dynamics of individual cells are described by a parametric differential equation [1,2]. Cell-to-cell variability is modeled by differences in parameter values of individual cells. These parameters are subject to a probability distribution, encoding the heterogeneity. This modeling framework allows the consideration of (epi-)genetic differences, and differences in cell states [1,2].
For this model class we present a Bayesian framework for estimating population heterogeneity from measurement data [1]. This framework allows the computation of the optimal parameter distribution via convex optimization. Uncertainty of the estimate is assessed using confidence intervals which are determined via an efficient Markov-chain Monte Carlo sampling algorithm.
To evaluate the approach a model of the TNF signaling pathway is analyzed. Artificial data are generated and the cell-to-cell variability is reconstructed.

Conclusions: The introduced framework offers the possibility of computationally efficiently assessment of the model uncertainties. Thus, it enables the evaluation of the predictive power of a model and hence the validity of the drawn conclusions. Furthermore, the study of the TNF signaling pathway illustrates the superiority of Bayesian methods and exemplifies the role of the usage of prior information, and the information content of flow cytometry data for studying population heterogeneity.

References:
[1] S.L. Spencer et al. (2009) Nature 459(7245):428-433.
[2] J. Hasenauer et al. (2011) BMC Bioinformatics 12:125.},
	Author = {J. Hasenauer and S. Waldherr and M. Doszczak and N. Radde and P. Scheurich and F. Allg\"ower},
	Date-Added = {2011-09-05 12:49:16 +0200},
	Date-Modified = {2013-11-04 21:21:11 +0100},
	Howpublished = {Abstract in Proc. of 12th Int. Conf. Systems Biology (ICSB 2011), August 28 - September 1, Heidelberg/Mannheim, Germany},
	Keywords = {cell population, parameter estimation, uncertainty analysis, dynamical systems},
	Month = {Aug.},
	Rating = {5},
	Read = {1},
	Title = {Parameter estimation and uncertainty analysis for models of heterogeneous cell populations},
	Year = {2011},
	Bdsk-File-1 = {}}

@article{Trucco1965,
	Abstract = {P. B. M. Walker (1954) and H. C. Longuet-Higgins (quoted by Walker), as well as O. Scherbaum and G. Rasch (1957), made the first attempts towards a mathematical study of the age distribution in a cellular population. It was H. Von Foerster (1959), however, who derived the complete differential equation for the age density function, n(t, a). His equation is obtained from an analysis of the inBnitesimal changes occurring during a time element dt in a group of cells with ages between a and a + da. The behavior of the population is determined by a quantity A which we call the loss function. In this paper a rigorous discussion of the Von Foerster equation is presen l, and a solution is given for the special case when A depends on t (time) and a (age) but not on other variables (such as n itself). It is also shown that the age density, n(t, a), is completely known only if the birth rate, a(t), and the initial age distribution, (a), are given as boundary conditions. In Section 1I the steady state solution and some plausible forms of intrinsic loss functions (depending on a only) are discussed in view of later applications.},
