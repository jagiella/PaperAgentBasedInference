	Journal = {Bioinf.},
	Keywords = {network reconstruction, variational Bayesian, ensemble learning, mean field approximation},
	Month = {Apr.},
	Number = {3},
	Pages = {349--356},
	Rating = {4},
	Read = {1},
	Title = {A {B}ayesian approach to reconstructing genetic regulatory networks with hidden factors},
