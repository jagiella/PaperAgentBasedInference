	Journal = {Nat. Comm.},
	Keywords = {stochastic modeling, moment equation, inversion effect},
	Month = {Apr.},
	Number = {779},
	Title = {Discreteness-induced concentration inversion in mesoscopic chemical systems},
	Volume = {3},
	Year = {2012},
	Bdsk-File-1 = {},
