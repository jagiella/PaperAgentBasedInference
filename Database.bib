	Editor = {H. Koeppl and J. A\'cimovi\'c and J. Kesselin and T. M\"aki-Marttunen},
	Howpublished = {Abstract in Proc. of Workshop on Comp. Syst. Biol. (WCSB 2011), June 6-8, , 2011 Z\"urich, Switzerland},
	Keywords = {kernel density estimation, population, heterogeneity, neurobiology, dynamical systems},
	Month = {June},
	Organization = {Tampere International Center for Signal Processing},
	Pages = {209},
	Rating = {5},
	Read = {1},
	Series = {TICSP series \# 57},
	Title = {A threshold-free method for assessing the responsiveness of heterogeneous populations: {D}RG-neurons as a case study},
	Year = {2011},
