	Keywords = {TNF, TRAIL, signaling, receptor},
	Month = {June},
	Number = {23},
	Pages = {16576--16587},
	Rating = {5},
	Read = {1},
	Title = {Dominant negative effects of {TNF}-related apoptosis-inducing ligand ({TRAIL}) receptor 4 on {TRAIL} receptor 1 signaling by formation of heteromeric complexes},
	Volume = {289},
	Year = {2014},
