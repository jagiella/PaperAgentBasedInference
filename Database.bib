@misc{SchittlerWal2010,
	Abstract = {The cell differentiation process corresponds to a loss of multipotency and increasing specialization. This can be viewed as a sequence of cell fate decisions, each represented by a genetic switch. In this work we focus on one such decision step in the differentiation of mesenchymal stem cells (MSCs) into osteoblasts. We develop a model for the "osteo-chondro switch", i.e. the differentiation of osteo-chondro progenitors into either osteoblasts or chondrocytes.

For this genetic switch, a set of key transcription factors (TFs) determines whether the cell state corresponds to the progenitor, osteogenic, or chondrogenic cell fate: Tweak, known as a pro-proliferative, differentiation-inhibiting TF; Osx and Sox9 as an osteogenic and a chondrogenic marker, respectively. Ordinary differential equations are used to describe the interactions between these TFs, thus the state of the cell system. 

Our model accounts for extrinsic and intrinsic signals which cells are exposed to in differentiation assays. Extrinsic stimuli are pro-differentiation, pro-osteogenic or pro-chondrogenic stimuli, or combinations of those. Intrinsic signals comprise individual cell properties subject to stochasticity and heterogeneity within cell populations. Both together determine the eventual cell fate. By analyzing different signals, we aim to find combinations, amounts and orders of stimuli which optimize the differentiation outcome.

As we demonstrate by our model, the response of individual cells upon the same stimulus can differ due to heterogeneous parameters. This imposes a limit on the maximum differentiation success rate. Despite several possible stimulation protocols leading to the same accumulated input, the system response might not be equal but depending on the order of stimuli and their time course.

The presented model captures the differentiation process of osteo-chondro progenitor cells and provides a framework to investigate the effects of stimuli on the cell system.},
	Author = {D. Schittler and S. Waldherr and J. Hasenauer and F. Allg\"ower},
	Date-Added = {2010-06-22 21:52:28 +0200},
	Date-Modified = {2013-11-04 21:40:48 +0100},
	Howpublished = {3rd Conference on Systems Biology of Mammalian Cells (SBMC 2010), Freiburg, Germany},
	Keywords = {gene regulation network, cell differentiation, osteoblast, dynamical systems},
	Month = {June},
	Note = {Abstract},
	Rating = {5},
	Read = {1},
	Title = {Modeling genetic switching in osteoblast cell fate driven by extrinsic and intrinsic signals},
	Year = {2010},
	Bdsk-File-1 = {}}

@misc{HasenauerWal2009b,
	Abstract = {Modelling in systems biology often aims at achieving a quantitative description of intracellular signal transduction or differentiation processes at the cellular level. Most models describe a ''typical single cell'' on the basis of experimental data obtained from cell populations. However, in order to understand the dynamical behaviour within heterogeneous cell populations, a consideration of many cells within the whole population is crucial.

Phenotypic heterogeneity of individual cells within one cell population can have different reasons. The most important ones are randomness in biochemical reactions, unequal partitioning of cellular material at cell division, and epigenetic differences. When considering cells with high mutation rate, such as cancer cells, also genotypic heterogeneity can play a major role. Here, we choose to model heterogeneity by differences in parameter values of the model describing the process of interest. The model structure is on the contrary assumed to be identical in all cells, as it usually represents the physical interactions among molecules, which should be independent of the cell's state. This parametric approach is well suited for genetic and epigenetic differences. The distribution of parameter values within the considered cell population is described by a suitable multivariate probability distribution function, which needs to be part of the model specification. 

Mathematical modelling of such cell populations will typically result in a non-linear partial differential equation for the probability distributions of the state variables. Since this is very hard to deal with, we propose to use a sample based approach, consisting of a large collection of ordinary differential equation systems of identical structure, but with differing parameter values which are subject to the specified parameter distribution function.

We present a heuristic approach to estimate the parameter distribution from the distributions of measured concentrations within the cell population. Such data are available on a suitable scale for instance from a newly developed measurement technology, the flow cytometric fluorescence microscopy, which is a combination of classical flow cytometry and fluorescence microscopy. The algorithm used to estimate the parameter distributions consists of two steps. At first, simulation data are generated for a suitable choice of parameter samples. Using these samples in the second step a convex optimisation problem is constructed to compute a suitable weighting for the considered parameter samples. This obtained weighting can directly be transformed into a probability distribution for the parameter values. 

In order to illustrate the suggested estimation method it is applied to artificial data generated from a simple model of the TNF signal transduction pathway. For the parameters where heterogeneity is assumed, our method yields good estimates of the parameter distribution function of the underlying model.

References
S. Waldherr, J. Hasenauer and F. Allg{\"o}wer (2009). Estimation of biochemical network parameter distributions in cell populations. Proceedings of the 15th IFAC Symposium on System Identification, (accepted)},
	Author = {J. Hasenauer and S. Waldherr and F. Allg\"ower},
	Date-Added = {2010-06-22 17:56:18 +0200},
	Date-Modified = {2013-11-04 21:20:21 +0100},
	Howpublished = {Workshop on Cancer Systems Biology, Rostock, Germany},
	Keywords = {cell population, cell-to-cell variability, parameter estimation, dynamical systems},
	Month = {May},
	Note = {Abstract},
	Rating = {5},
	Read = {1},
	Title = {Estimation of biochemical network parameter distributions in cell populations},
	Year = {2009},
	Bdsk-File-1 = {}}

@misc{HasenauerWal2008,
	Abstract = {Objective: In this work we analyze uncertain biochemical reaction networks modelled by ordinary differential equations. The goal is to compute outer bounds for the region of feasible equilibrium points when parameters are allowed to vary in a certain range.

Results: A relaxed version of the corresponding feasibility problem and its Lagrangian dual is used to show how to compute certificates for regions in state space not containing any steady states. The calculation of this certificates can be implemented via semidefinite programming which in combination with a modified bisection algorithm allows an efficient computation of an outer approximation of the region of all feasible equilibrium points.
These bounds are then used to define a global sensitivity measure which in the limit of small parameter uncertainties approaches the absolute value of the first-order sensitivity. In the case of larger uncertainties, the measure indicates effects such as for which perturbation a saturation is reached, or whether there are strong nonlinear coupling effects.
Furthermore, we show that this approach is applicable to complex intracellular signalling networks by applying it to a model of the tumor necrosis factor (TNF) signalling pathway. The results of our sensitivity analysis indicate a strong influence of TNF receptor 2 to TNF receptor 1 signalling via the adaptor protein TRAF2, whereas the influence from TNF receptor 1 to TNF receptor 2 is negligible.

Conclusion: The presented method for global sensitivity analysis is computational tractable for medium-size systems and can detect nonlinear effects like saturation and coupling.},
	Author = {J. Hasenauer and S. Waldherr and F. Allg\"ower},
	Date-Added = {2010-06-22 17:50:41 +0200},
	Date-Modified = {2013-11-04 21:20:15 +0100},
	Howpublished = {Book of Abstracts of International Conference on Systems Biology (ICSB 2008), Gothenburg, Sweden},
	Keywords = {semidefinite programming, uncertainty analysis, dynamical systems},
	Month = {Sept.},
	Note = {Abstract},
	Rating = {5},
	Read = {1},
	Title = {Global sensitivity analysis of biochemical reaction networks using semidefinite programming},
	Year = {2008},
	Bdsk-File-1 = {}}

@misc{HasenauerDos2010,
	Abstract = {In many situations of biological relevance individual cells within a clonal population show
different responses upon a common stimulus. One example are populations of cancer
cells where some cells die upon a death signal and others survive. Although the decision
at the single cell level is binary a continuous response can be observed at the population
scale.

Within this project a population of NCI-H460 tumor cells is studied. This population is
highly heterogeneous as single cells of this population differ in protein expression levels
and sensitivities with respect to the cytokine TRAIL. To achieve a better understanding of
TRAIL-induced apoptosis in a cell population a multi-scale model is developed.

This mathematical model describes TRAIL signaling in single-cells on the basis of
mechanistic ordinary differential equations, whereas the population is modeled by an
ensemble of individual cells. This leads to a partial differential equation governing the
population dynamics. Cell-to-cell variability is introduced by differences in protein synthesis
rates among individual cells, interpretable as epigenetic differences. The distribution of the
synthesis rates is estimated from data obtained by flow cytometry.

We show that epigenetic differences can explain the distribution of death instances
observed in experiments. Plausible markers for the survival or death of individual cells
could be determined. The model indicates that e.g. for high TRAIL concentrations mainly
differences in procaspase 3 and XIAP expression levels play crucial roles for the cell fate
decision, whereas for low TRAIL concentrations also the procaspase 8 expression level
might represent an important marker. Variability within the mitochondrial pathway appears
negligible, which is in accordance with experimental data. The comparison of the model
predictions with the experimental data supports the predictive power achieved by the
combination of the single-cell model and cell population heterogeneity.},
	Author = {J. Hasenauer and M. Doszczak and S. Borchers and S. Waldherr and R. Findeisen and P. Scheurich and F. Allg\"ower},
	Date-Added = {2010-06-22 10:58:08 +0200},
	Date-Modified = {2013-11-04 21:17:48 +0100},
	Howpublished = {3rd Conference on Systems Biology of Mammalian Cells (SBMC 2010), Freiburg, Germany},
	Keywords = {cell population, heterogeneity, parameter estimation, dynamical systems},
	Month = {June},
	Note = {Abstract},
	Rating = {5},
	Read = {1},
	Title = {Single-cells vs. cell populations - From a binary decision to a continuous response},
	Year = {2010},
	Bdsk-File-1 = {}}

@article{PaszekRya2010,
	Abstract = {Heterogeneity between individual cells is a common feature of dynamic cellular processes, including signaling, transcription, and cell fate; yet the overall tissue level physiological phenotype needs to be carefully controlled to avoid fluctuations. Here we show that in the NF-κB signaling system, the precise timing of a dual-delayed negative feedback motif [involving stochastic transcription of inhibitor κB (IκB)-α and -ε] is optimized to induce heterogeneous timing of NF-κB oscillations between individual cells. We suggest that this dual-delayed negative feedback motif enables NF-κB signaling to generate robust single cell oscillations by reducing sensitivity to key parameter perturbations. Simultaneously, enhanced cell heterogeneity may represent a mechanism that controls the overall coordination and stability of cell population responses by decreasing temporal fluctuations of paracrine signaling. It has often been thought that dynamic biological systems may have evolved to maximize robustness through cell-to-cell coordination and homogeneity. Our analyses suggest in contrast, that this cellular variation might be advantageous and subject to evolutionary selection. Alternative types of therapy could perhaps be designed to modulate this cellular heterogeneity.},
	Author = {P. Paszek and S. Ryan and L. Ashall and K. Sillitoe and C. V. Harper and D. G. Spiller and D. A. Rand and M. R. H. White},
