	Journal = {J. Theor. Biol.},
	Keywords = {branching process},
	Month = {May},
	Number = {1},
	Pages = {7--18},
	Title = {Multi-type branching models to describe cell differentiation programs},
	Volume = {277},
	Year = {2011},
