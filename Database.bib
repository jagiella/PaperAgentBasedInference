	Pdf = {model reduction/Conzelmann2006.pdf},
	Rating = {5},
	Read = {1},
	Title = {A domain-oriented approach to the reduction of combinatorial complexity in signal transduction networks},
	Url = {http://www.biomedcentral.com/1471-2105/7/34},
