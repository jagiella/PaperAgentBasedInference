	Doi = {10.1186/1752-0509-6-120},
	Journal = {{BMC} Syst. Biol.},
	Keywords = {uncertainty analysis, profile likelihood, uncertainty analysis, maximum likelihood},
	Month = {Sept.},
	Number = {120},
	Rating = {5},
	Read = {1},
	Title = {Likelihood based observability analysis and confidence intervals for predictions of dynamic models},
	Volume = {6},
	Year = {2012},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1186/1752-0509-6-120}}

