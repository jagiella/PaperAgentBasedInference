@article{EissingCon2004,
	Abstract = {Apoptosis is an important physiological process crucially involved in development and homeostasis of multicellular organisms. Although the major signaling pathways have been unraveled, a detailed mechanistic understanding of the complex underlying network remains elusive. We have translated here the current knowledge of the molecular mechanisms of the death-receptor-activated caspase cascade into a mathematical model. A reduction down to the apoptotic core machinery enables the application of analytical mathematical methods to evaluate the system behavior within a wide range of parameters. Using parameter values from the literature, the model reveals an unstable status of survival indicating the need for further control. Based on recent publications we tested one additional regulatory mechanism at the level of initiator caspase activation and demonstrated that the resulting system displays desired characteristics such as bistability. In addition, the results from our model studies allowed us to reconcile the fast kinetics of caspase 3 activation observed at the single cell level with the much slower kinetics found at the level of a cell population.},
	Author = {T. Eissing and H. Conzelmann and E.D. Gilles and F. Allg{\"o}wer and E. Bullinger and P. Scheurich},
	Date-Modified = {2011-12-23 17:31:48 +0100},
