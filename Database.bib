	Doi = {10.3389/fphys.2011.00004},
	Journal = {Front. Physio.},
	Keywords = {modeling, multi-scale modeling, pharmacokinetics, cancer, cancer treatment},
	Month = {Feb.},
	Number = {4},
	Rating = {4},
	Read = {1},
